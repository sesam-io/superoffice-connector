                  "add",
                  "$discard",
                  "already inserted"
                ]
              ],
              [
                "if",
                [
                  "is-not-empty",
                  "_T.SaleId"
                ],
                [
                  [
                    "add-if",
                    "_QuoteId",
                    [
                      "first",
                      [
                        "hops",
                        {
                          "datasets": [
                            "{{@ system @}}-quote-collect qc"
                          ],
                          "return": "qc.QuoteId",
                          "where": [
                            [
                              "eq",
                              "_S.sesam_SaleId",
                              "qc.sesam_SaleId"
                            ]
                          ]
                        }
                      ]
                    ]
                  ],
                  [
                    "if",
                    [
                      "is-not-empty",
                      "_T._QuoteId"
                    ],
                    [
                      [
                        "add",
                        "$operation",
                        "lookup-quote"
                      ],
                      [
                        "add",
                        "payload",
                        [
                          "dict",
                          "QuoteId",
                          "_T._QuoteId"
                        ]
                      ]
                    ],
                    [
                      "add",
                      "$discard",
                      "missing QuoteId"
                    ]
                  ]
                ],
                [
                  "add",
                  "payload",
                  "_S."
                ]
              ]
            ]
          ]
        },
        "type": "dtl"
      },
