[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "source": {
      "dataset": "{{@ system @}}-{{@ parent @}}-collect",
      "type": "dataset"
    },
    "transform": {
      "rules": {
        "create_versions": [
          [
            "copy",
            "*"
          ],
          [
            "add",
            "$SaleId",
            "_R._S.SaleId"
          ],
          [
            "add",
            "_id",
            [
              "string",
              "_S.QuoteVersionId"
            ]
          ],
          [
            "add",
            "$original_id",
            "_R._S._id"
          ]
        ],
        "default": [
          [
            "create-child",
            [
              "apply",
              "create_versions",
              "_S.QuoteVersions"
            ]
          ]
        ]
      },
      "type": "dtl"
    },
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all2",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "type": "dataset"
    },
    "transform": [
      {
        "type": "emit_children"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all2",
      "type": "dataset"
    },
    "transform": {
      "rules": {
        "default": [
          [
            "copy",
            "*"
          ],
          [
            "add",
            "$last-modified",
            [
              "datetime-parse",
              "%Y-%m-%dT%H:%M:%S",
              "_S.Updated"
            ]
          ]
        ]
      },
      "type": "dtl"
    },
    "type": "pipe"
  }
]