{
  "_id": "{{@ system @}}-{{@ datatype @}}-collect",
  "metadata": {
    "supports_since": true
  },
  "namespaced_identifiers": false,
  "source": {
    "operation": "list",
    "properties": {
      "base_url": "{{@ datatype @}}",
      "ordering": "PrimaryKey asc",
      "primary_key": "PrimaryKey",
      "skip": "0",
      "top": "20",
      "updated_param": "updatedDate"
    },
    "since_property_name": "since",
    "supports_since": true,
    "system": "{{@ system @}}",
    "type": "rest",
    "updated_expression": "{{ updatedDate }}"
  },
  "transform": {
    "rules": {
      "default": [
        [
          "copy",
          "*"
        ],
        [
          "add",
          "$last-modified",
          [
            "datetime-parse",
            "%Y-%m-%dT%H:%M:%S",
            "_S.updatedDate"
          ]
        ]
      ]
    },
    "type": "dtl"
  },
  "type": "pipe"
}