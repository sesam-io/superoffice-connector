{
  "_id": "{{@ system @}}-person-collect",
  "namespaced_identifiers": false,
  "source": {
    "dataset": "{{@ system @}}-personlist-collect",
    "type": "dataset"
  },
  "transform": [
    {
      "operation": "lookup",
      "properties": {
        "datatype": "person"
      },
      "replace_entity": true,
      "system": "{{@ system @}}",
      "type": "rest"
    },
    {
      "rules": {
        "default": [
          [
            "copy",
            "*"
          ],
          [
            "add",
            "_id",
            [
              "string",
              "_S.PersonId"
            ]
          ]
        ]
      },
      "type": "dtl"
    }
  ],
  "type": "pipe"
}