                    "_S.$source.$payload",
                    [
                      "apply",
                      "payload",
                      "_S.$source"
                    ]
                  ]
                ],
                [
                  "add",
                  "payload",
                  "_T._payload"
                ]
              ]
            },
            "type": "dtl"
          },
          {
            "allowed_status_codes": "100-599",
            "operation": "{{@ datatype @}}-default",
