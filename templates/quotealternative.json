                  "copy",
                  "*",
                  [
                    "list",
                    "$trace_*",
                    "$source"
                  ]
                ]
              ],
              "update": [
                [
                  "add",
                  "$operation",
                  "update"
                ],
                [
                  "add",
                  "_payload",
                  [
                    "if",
                    [
                      "is-not-null",
                      "_S.$source.$payload"
