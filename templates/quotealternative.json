[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ parent @}}-collect",
      "type": "dataset"
    },
    "transform": {
      "rules": {
        "create_versions": [
          [
            "copy",
            "*"
          ],
          [
            "add",
            "$SaleId",
            "_R._S.$SaleId"
          ],
          [
            "add",
            "_id",
            [
              "string",
              "_S.QuoteAlternativeId"
            ]
          ],
          [
            "add",
            "$original_id",
            "_R._S._id"
          ],
          [
            "add",
            "$last-modified",
            "_R._S.$last-modified"
          ]
        ],
        "default": [
          [
            "create-child",
            [
              "apply",
              "create_versions",
              "_S.QuoteAlternatives"
            ]
          ]
        ]
      },
      "type": "dtl"
    },
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all2",
    "namespaced_identifiers": false,
    "source": {
