                  "add",
                  "$operation",
                  "insert"
                ],
                [
                  "add",
                  "_payload",
                  [
                    "if",
                    [
                      "is-not-null",
                      "_R._T.$source.$payload"
                    ],
                    "_R._T.$source.$payload",
                    [
                      "apply",
                      "payload",
                      "_R._T.$source"
                    ]
                  ]
                ],
                [
                  "add",
                  "payload",
                  "_T._payload"
                ]
              ],
              "lookup": [
                [
                  "add",
                  "$operation",
                  "lookup"
                ]
              ],
              "payload": [
                [
                  "merge",
                  [
                    "dict",
                    [
                      "filter",
                      [
                        "not",
                        [
                          "or",
                          [
                            "matches",
                            "_*",
                            [
                              "first",
                              "_."
                            ]
                          ],
                          [
                            "matches",
                            "$*",
                            [
                              "first",
                              "_."
                            ]
                          ]
                        ]
                      ],
                      [
                        "items",
                        "_S."
                      ]
                    ]
                  ]
                ]
              ]
            },
            "type": "dtl"
          },
          {
            "allowed_status_codes": "100-599",
            "operation": "{{@ datatype @}}-lookup",
