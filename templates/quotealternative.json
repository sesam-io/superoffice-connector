                      [
                        "apply",
                        "payload",
                        "_S.$source"
                      ],
                      "b",
                      "_S.$source.$based_on"
                    ]
                  ]
                ]
              ],
              "based-on-update": [
                [
                  "add",
                  "$based_on_update",
                  [
                    "apply",
                    "based-on-extract",
                    [
                      "dict",
                      "a",
                      [
                        "apply",
                        "payload",
                        "_S.$source"
                      ],
                      "b",
                      [
                        "apply",
                        "payload",
                        "_S.$source"
                      ]
                    ]
                  ]
                ]
              ],
              "default": [
                [
                  "if",
                  [
                    "neq",
                    "_S.$operation",
                    "lookup"
                  ],
                  [
                    [
                      "if",
                      [
                        "eq",
                        "_S.$operation",
                        "insert"
                      ],
                      [
                        "if",
                        [
                          "is-null",
                          "_S.$origin"
                        ],
                        [
                          "fail!",
                          "Entity to be inserted does not have $origin property"
                        ]
                      ]
                    ],
                    [
                      "copy",
                      "*"
                    ]
                  ],
                  [
                    [
                      "if",
                      [
                        "neq",
                        "_S.response_status",
                        200
                      ],
                      [
                        "copy",
                        "*"
                      ],
                      [
                        [
                          "add",
                          "$comparison",
                          [
                            "apply",
                            "based-on-comparison",
                            "_S."
                          ]
                        ],
                        [
                          "case-eq",
                          [
                            "path",
                            "$result",
                            "_T.$comparison"
                          ],
                          "modified-in-system",
                          [
                            "merge",
                            [
                              "apply",
                              "discard-modified-in-system",
                              "_S."
                            ]
                          ],
                          "entity-not-changed",
                          [
                            "merge",
                            [
                              "apply",
                              "discard-entity-not-changed",
                              "_S."
                            ]
                          ],
                          [
                            "merge",
                            [
                              "apply",
                              "update",
                              "_S."
                            ]
                          ]
                        ],
                        [
                          "copy",
                          "$source"
                        ],
                        [
                          "copy",
                          [
                            "literal",
                            "QuoteAlternativeId"
                          ]
                        ],
                        [
                          "copy",
                          "$origin"
                        ],
                        [
                          "copy",
                          "$replaced"
                        ],
                        [
                          "add",
                          "$trace_lookup",
                          [
                            "apply",
                            "trace",
                            "_S."
                          ]
                        ]
                      ]
                    ]
                  ]
                ]
              ],
              "discard-entity-not-changed": [
                [
                  "add",
                  "$operation",
                  "discard-entity-not-changed"
                ]
              ],
              "discard-modified-in-system": [
                [
                  "add",
                  "$operation",
                  "discard-modified-in-system"
                ]
              ],
              "payload": [
                [
                  "merge",
                  [
                    "dict",
                    [
                      "filter",
                      [
                        "not",
                        [
                          "or",
                          [
                            "matches",
                            "_*",
                            [
                              "first",
                              "_."
                            ]
                          ],
                          [
                            "matches",
                            "$*",
                            [
                              "first",
                              "_."
                            ]
                          ]
                        ]
                      ],
                      [
                        "items",
                        "_S."
                      ]
                    ]
                  ]
                ]
              ],
              "trace": [
