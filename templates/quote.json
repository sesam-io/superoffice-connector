[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "exclude_completeness": [
      "{{@ system @}}-{{@ datatype @}}-share"
    ],
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-salelistQuote-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "operation": "{{@ datatype @}}-lookup",
        "properties": {
          "datatype": "GetQuoteEntity"
        },
        "replace_entity": true,
        "system": "{{@ system @}}",
        "trace": true,
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "_id",
              [
                "string",
                "_S.QuoteId"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "primary_key": "QuoteId",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-collect-rest",
        "type": "template"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-salelist",
    "namespaced_identifiers": false,
    "source": {
      "operation": "sale{{@ datatype @}}-list",
      "properties": {
        "datatype": "sale",
        "ordering": "name asc",
        "properties": "select={{@ datatype @}}/{{@ datatype @}}Id,updatedDate",
        "since_param": "updatedDate",
        "skip": "0",
        "top": "1000"
      },
      "since_property_name": "updatedDate",
      "supports_since": true,
      "system": "{{@ system @}}",
      "type": "rest",
      "updated_expression": "{{ updatedDate }}"
    },
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-share",
    "source": {
      "entities": [
        {
          "_id": "1"
        }
      ],
      "if_source_empty": "accept",
      "type": "embedded"
    },
    "type": "pipe"
  }
]