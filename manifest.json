{
  "additional_parameters": {
    "account_id": {},
    "base_url": {},
    "service_url": {}
  },
  "auth": "oauth2",
  "auth_variant": "superoffice-ticket",
  "datatypes": {
    "contact": {
      "label": "Company",
      "parameters": {
        "base_url": "Contact",
        "list_url": "contactId",
        "primary_key": "ContactId",
        "updated_param": "updatedDate"
      },
      "template": "templates/contact.json",
      "webhook": true
    },
    "listcountryitems": {
      "label": "Country Items List",
      "parameters": {
        "base_url": "List/Country/Items",
        "lookup_id": "CountryId"
      },
      "sync_frequency": "weekly",
      "template": "templates/listcountryitems.json"
    },
    "listcurrencyitems": {
      "label": "Currency Items List",
      "parameters": {
        "base_url": "List/Currency/Items",
        "lookup_id": "CurrencyId"
      },
      "sync_frequency": "weekly",
      "template": "templates/listcurrencyitems.json"
    },
    "ownercontactlink": {
      "label": "Owner Contact Link",
      "sync_frequency": "slow",
      "template": "templates/ownercontactlink.json"
    },
    "person": {
      "label": "Contact Person",
      "parameters": {
        "base_url": "Person",
        "list_url": "personId",
        "primary_key": "PersonId",
        "updated_param": "personUpdatedDate"
      },
      "template": "templates/contact.json",
      "webhook": true
    },
    "pricelist": {
      "label": "Price",
      "parameters": {
        "base_url": "PriceList",
        "list_url": "PrimaryKey",
        "primary_key": "PriceListId",
        "updated_param": "updatedDate"
      },
      "template": "templates/pricelist.json"
    },
    "product": {
      "label": "Product",
      "parameters": {
        "base_url": "Product",
        "list_url": "productId",
        "primary_key": "productId",
        "updated_param": "updatedDate"
      },
      "template": "templates/product.json"
    },
    "quote": {
      "label": "Quote",
      "template": "templates/quote.json",
      "webhook": true
    },
    "quotealternative": {
      "label": "Quote Alternative",
      "parent": "quoteversion",
      "template": "templates/quotealternative.json"
    },
    "quotealternative-quote": {
      "template": "templates/quotealternative-quote.json"
    },
    "quotealternative-sale": {
      "template": "templates/quotealternative-sale.json"
    },
    "quoteline": {
      "label": "Quote Line",
      "parent": "quotealternative",
      "template": "templates/quoteline.json"
    },
    "quoteversion": {
      "label": "Quote Version",
      "parent": "quote",
      "template": "templates/quoteversion.json"
    },
    "sale": {
      "template": "templates/sale.json"
    },
    "user": {
      "label": "User",
      "sync_frequency": "weekly",
      "template": "templates/user.json"
    },
    "webhook": {
      "template": "templates/webhook.json"
    }
  },
  "oauth2": {
    "login_url": "https://sod.superoffice.com/login/common/oauth/authorize",
    "scopes": [
      "openid",
      "api"
    ],
    "token_url": "https://sod.superoffice.com/login/common/oauth/tokens"
  },
  "requires_service_api_access": true,
  "system-template": "templates/system.json",
  "use_webhook_secret": true
}